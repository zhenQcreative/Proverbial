    {
        "chapter":7,
        "verse": "Proverbs 7:19",
        "verseID": "Proverbs_7_5THRU20",
        "imageUrl": "img/img-category/",
        "category": "Adultery, Seduction",
        "categoryID": "NA",
        "text": "19 For my husband is not at home; He has gone on a long journey;"
